<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.cn.youyi.entity.Game" table="game" schema="gamesale">
        <id name="gid">
            <column name="gid" sql-type="int(5)"/>
        </id>
        <property name="gname">
            <column name="gname" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <set name="commoditiesByGid" inverse="true">
            <key>
                <column name="gid" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.cn.youyi.entity.Commodity"/>
        </set>
    </class>
</hibernate-mapping>
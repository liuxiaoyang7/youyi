<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.cn.youyi.entity.Oorder" table="oorder" schema="gamesale">
        <id name="oid">
            <column name="oid" sql-type="int(5)"/>
        </id>
        <property name="orderState">
            <column name="order_state" sql-type="int(5)" not-null="true"/>
        </property>
        <property name="buyTime">
            <column name="buy_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="userid">
            <column name="userid" sql-type="int(5)" not-null="true"/>
        </property>
        <property name="cid">
            <column name="cid" sql-type="int(5)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
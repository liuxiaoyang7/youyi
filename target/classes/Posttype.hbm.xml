<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.cn.youyi.entity.Posttype" table="posttype" schema="gamesale">
        <id name="ptid">
            <column name="ptid" sql-type="int(5)"/>
        </id>
        <property name="ptype">
            <column name="ptype" sql-type="varchar(8)" length="8" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>